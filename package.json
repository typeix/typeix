{
  "name": "typeix-srcs",
  "private": true,
  "version": "8.1.0",
  "description": "typeix helper functions",
  "scripts": {
    "coveralls-merge": "npm run test && node coveralls-merge.js",
    "bootstrap": "lerna exec npm install",
    "clean:lock": "lerna exec rm package-lock.json",
    "clean": "lerna exec rimraf dist",
    "test": "lerna exec npm test",
    "publish:all": "lerna publish --force-publish --loglevel=verbose--loglevel=verbose",
    "publish:beta": "lerna publish --force-publish --dist-tag=beta --loglevel=verbose",
    "publish:next": "lerna publish --force-publish --dist-tag=next --loglevel=verbose",
    "publish:rc": "lerna publish --force-publish --dist-tag=rc --loglevel=verbose",
    "publish:test": "lerna publish --force-publish --dist-tag=test --loglevel=verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typeix/typeix.git"
  },
  "author": "Igor Ivanovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typeix/typeix/issues"
  },
  "homepage": "https://typeix.com",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.7",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">= 12.10.0"
  }
}
