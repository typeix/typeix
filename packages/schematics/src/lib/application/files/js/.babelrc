{
  "presets": ["@babel/preset-env"],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    [
      "babel-plugin-root-import",
      {
        "rootPathPrefix": "~/*",
        "rootPathSuffix": "src/*"
      }
    ]
  ]
}
