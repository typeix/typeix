{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "author": "<%= author %>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "typeix build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "typeix start",
    "start:dev": "typeix start --watch",
    "start:debug": "typeix start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch --config ./jest-runner.json",
    "test:cov": "jest --coverage --config ./jest-runner.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest-runner.json",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@typeix/resty": "^8.0.2"
  },
  "devDependencies": {
    "@typeix/cli": "^8.0.2",
    "@typeix/schematics": "^8.0.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-loader": "9.1.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "ttypescript": "^1.5.12",
    "typescript-transform-paths": "^2.2.3",
    "pino-pretty": "^4.7.1"
  },
  "engines": {
    "node": ">=12.9.0"
  }
}
