{
  "name": "@typeix/schematics",
  "version": "7.5.0",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postbuild": "npm run copy:collection && npm run copy:lib",
    "build": "rm -rf dist && tsc --project tsconfig.lib.json",
    "clean": "",
    "copy:collection": "cpx src/collection.json dist && cpx 'src/lib/**/schema.json' dist/lib",
    "copy:lib": "cpx 'src/lib/**/{files,workspace}/**/*.*' dist/lib && cpx 'src/lib/**/{files,workspace}/**/.!(gitignore)' dist/lib",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "npm run clean && NODE_ENV=test jest -w 1 --no-cache --config jest-runner.json",
    "test:dev": "NODE_ENV=test npm run -s test -- --watchAll",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-angular": "12.1.1",
    "@typeix/resty": "^7.4.1",
    "@types/fs-extra": "9.0.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.7",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "coveralls": "^3.1.0",
    "cpx": "^1.5.0",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "release-it": "^14.6.1",
    "ts-jest": "^26.5.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^2.2.3"
  },
  "dependencies": {
    "@angular-devkit/core": "11.2.4",
    "@angular-devkit/schematics": "11.2.4",
    "fs-extra": "9.1.0",
    "jsonc-parser": "3.0.0",
    "pluralize": "8.0.0"
  },
  "engines": {
    "node": ">=12.9.0"
  },
  "schematics": "./dist/collection.json",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typeix/schematics.git"
  },
  "author": "Igor Ivanovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typeix/schematics/issues"
  }
}
