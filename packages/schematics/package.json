{
  "name": "@typeix/schematics",
  "version": "8.0.6",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postbuild": "npm run copy:collection && npm run copy:lib",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.lib.json",
    "copy:collection": "cpx src/collection.json dist && cpx 'src/lib/**/schema.json' dist/lib",
    "copy:lib": "cpx 'src/lib/**/{files,workspace}/**/*.*' dist/lib && cpx 'src/lib/**/{files,workspace}/**/.!(gitignore)' dist/lib",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "pretest": "rimraf dist && tsc --project tsconfig.json",
    "test": "jest --config jest-runner.json --coverage",
    "test:dev": "npm run -s test -- --watchAll",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-angular": "12.1.1",
    "@typeix/resty": "^8.0.6",
    "@types/fs-extra": "9.0.11",
    "@types/jest": "^26.0.23",
    "cpx": "^1.5.0",
    "ts-jest": "^26.5.3",
    "ttypescript": "^1.5.12",
    "typescript-transform-paths": "^2.2.3"
  },
  "dependencies": {
    "@angular-devkit/core": "11.2.4",
    "@angular-devkit/schematics": "11.2.4",
    "fs-extra": "9.1.0",
    "jsonc-parser": "3.0.0",
    "pluralize": "8.0.0"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "schematics": "./dist/collection.json",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typeix/schematics.git"
  },
  "author": "Igor Ivanovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typeix/schematics/issues"
  }
}
